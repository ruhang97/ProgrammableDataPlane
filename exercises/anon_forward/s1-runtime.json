{
  "target": "bmv2",
  "p4info": "build/anon_forward.p4.p4info.txt",
  "bmv2_json": "build/anon_forward.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:01:11",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:02:00",
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.3.3", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:00",
        "port": 3
      }
    },
    {
      "table": "MyEgress.anonForward_exact",
      "match": {
        "hdr.ipv4.srcAddr": "10.0.1.10",
        "hdr.tcp.srcPort": 5000
      },
      "action_name": "MyEgress.anonForward_set_src",
      "action_params": {
        "ipv4_srcAddr": "10.0.2.20",
        "tcp_srcPort": 5000,
        "port": 2
      }
    },
      {"table": "MyEgress.anonForward_exact",
      "match": {
        "hdr.ipv4.srcAddr": "10.0.1.11",
        "hdr.tcp.srcPort": 5000
      },
      "action_name": "MyEgress.anonForward_set_src",
      "action_params": {
        "ipv4_srcAddr": "10.0.2.20",
        "tcp_srcPort": 5001,
        "port": 2
      }
    }
  ]
}
